{
  "expo": {
    "name": "RoFits",
    "slug": "ro-fits",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#ee2424",
    "backgroundColor": "#ee2424",
    "icon": "./assets/images/icon.png",
    "scheme": "ro-fits",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ee2424"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#ee2424"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
